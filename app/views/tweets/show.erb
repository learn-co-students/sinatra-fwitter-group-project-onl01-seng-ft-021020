<h3><%= @tweet.content %></h3>

<form action='/tweets/<%=@tweet.id%>/edit' method="get">
  <input type='submit' id='edit' name='edit' value='Edit Tweet'>
</form>
<form action='/tweets' method="post">
  <input type='hidden' id='hidden' name='_method' value='delete'>
  <input type='hidden' id='hidden' name='tweet_id' value='<%=@tweet.id%>'>
  <input type='submit' id='delete' name='delete' value='Delete Tweet'>
</form>
